<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zh.springbootdocker.dao.AccountDao">

    <select id="findAccountById" parameterType="map" resultType="com.zh.springbootdocker.entity.Account">
        SELECT
        userName,
        money
        FROM account
        WHERE id = #{id}
    </select>


</mapper>
